# Domain & SSL
DOMAIN=location404.com.br
ACME_EMAIL=admin@location404.com.br

# Docker Registry (GitHub Container Registry)
DOCKER_REGISTRY=ghcr.io/location404
VERSION=main

# Google Maps
GOOGLE_MAPS_API_KEY=your-google-maps-api-key

# JWT (gere com: openssl rand -base64 64)
JWT_SIGNING_KEY=your-jwt-secret-key-here

# PostgreSQL Auth (externo - configurar manualmente)
POSTGRES_AUTH_CONNECTION=Host=seu-host-postgres;Port=5432;Database=location404_auth;Username=location404_auth;Password=sua-senha

# PostgreSQL Data (externo - configurar manualmente)
POSTGRES_DATA_CONNECTION=Host=seu-host-postgres;Port=5432;Database=location404_data;Username=location404_data;Password=sua-senha

# Dragonfly/Redis (externo - configurar manualmente)
REDIS_CONNECTION_STRING=seu-host-redis:6379,password=sua-senha

# RabbitMQ (externo - configurar manualmente)
RABBITMQ_HOST=seu-host-rabbitmq
RABBITMQ_PORT=5672
RABBITMQ_USER=location404
RABBITMQ_PASSWORD=sua-senha

# Grafana
GRAFANA_USER=admin
GRAFANA_PASSWORD=change-password
